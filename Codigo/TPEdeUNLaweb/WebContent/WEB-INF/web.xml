    <url-pattern>/MostrarClienteJSP</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ControladorListaCliente</servlet-name>
    <servlet-class>controladores.ControladorListaCliente</servlet-class>
  </servlet>
